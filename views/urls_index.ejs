<!DOCTYPE html>
<html lang= "en">
  <head>
    <title>TinyApp</title>
  </head>
  <body>
    <% include partials/_header %>
    <% for (var key in urls) { %>
       <h4><li> <%=key + ': ' + urls[key]%></li></h4> 
       &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<a href="/urls/<%= key%>/">Edit</a>
          <form method="POST" action="/urls/<%= key%>/delete" style="margin:15px;">
            <div class="button">
                <button type="submit">Delete 🗑️</button>
            </div>
          </form>
    <% } %>
    <a href="/urls/new/">Create a New Short Link</a>
  </body>
</html>
